programa 
{
  funcao inicio() 
  {
    inteiro vfinal, i
    escreva("Qual o valor final? ")
    leia(vfinal)
    escreva("Os números entre 1 e ", vfinal," são: ")
    para(i = 1; i <= vfinal; i++)
    {
      escreva (i, ", ")
    }
  }
}

